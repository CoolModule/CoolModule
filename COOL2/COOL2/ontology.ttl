@prefix : <http://www.w3id.org/arco/ontology/content#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.w3id.org/arco/ontology/content> .

<http://www.w3id.org/arco/ontology/content> rdf:type owl:Ontology ;
                                             owl:imports <https://w3id.org/arco/ontology/context-description> ,
                                                         <https://w3id.org/arco/ontology/denotative-description> .

#################################################################
#    Object Properties
#################################################################

###  http://www.w3id.org/arco/ontology/content/defines
<http://www.w3id.org/arco/ontology/content/defines> rdf:type owl:ObjectProperty ;
                                                    rdfs:comment "This property define the act of attaching a meaning to a term."@en .


###  http://www.w3id.org/arco/ontology/content/hasAuthor
<http://www.w3id.org/arco/ontology/content/hasAuthor> rdf:type owl:ObjectProperty ;
                                                      owl:inverseOf <http://www.w3id.org/arco/ontology/content/isAuthorOf> ;
                                                      rdfs:comment "This property links any written work to its creator or originator."@en .


###  http://www.w3id.org/arco/ontology/content/hasBusinessPartner
<http://www.w3id.org/arco/ontology/content/hasBusinessPartner> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isRelatedTo> ;
                                                               rdf:type owl:SymmetricProperty ;
                                                               rdfs:comment "This property defines a business relations bwtween two agents."@en .


###  http://www.w3id.org/arco/ontology/content/hasCharacter
<http://www.w3id.org/arco/ontology/content/hasCharacter> rdf:type owl:ObjectProperty ;
                                                         owl:inverseOf <http://www.w3id.org/arco/ontology/content/isCharacterOf> .


###  http://www.w3id.org/arco/ontology/content/hasCharacterRole
<http://www.w3id.org/arco/ontology/content/hasCharacterRole> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasType> ;
                                                             owl:inverseOf <http://www.w3id.org/arco/ontology/content/isCharacterRole> ;
                                                             rdfs:comment "This property defines character’s role as protagonist, antagonist or secondary."@en .


###  http://www.w3id.org/arco/ontology/content/hasCharacterScope
<http://www.w3id.org/arco/ontology/content/hasCharacterScope> rdf:type owl:ObjectProperty ;
                                                              rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasType> ;
                                                              owl:inverseOf <http://www.w3id.org/arco/ontology/content/isCharacterScope> ;
                                                              rdfs:comment "This property defines character’s scope as universal good, universal bad or self good."@en .


###  http://www.w3id.org/arco/ontology/content/hasCharacterStatus
<http://www.w3id.org/arco/ontology/content/hasCharacterStatus> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasType> ;
                                                               owl:inverseOf <http://www.w3id.org/arco/ontology/content/isCharacterStatus> ;
                                                               rdfs:comment "This property defines character’s status as leading, opposing or supporting."@en .


###  http://www.w3id.org/arco/ontology/content/hasCharacterType
<http://www.w3id.org/arco/ontology/content/hasCharacterType> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasType> ;
                                                             owl:inverseOf <http://www.w3id.org/arco/ontology/content/isCharacterType> ;
                                                             rdfs:comment "This property defines character’s type as hero, antihero, or villain."@en .


###  http://www.w3id.org/arco/ontology/content/hasChild
<http://www.w3id.org/arco/ontology/content/hasChild> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasRelative> ;
                                                     owl:inverseOf <http://www.w3id.org/arco/ontology/content/hasParent> ;
                                                     rdfs:comment "This property links an agent that is parent to another agent."@en .


###  http://www.w3id.org/arco/ontology/content/hasEmployee
<http://www.w3id.org/arco/ontology/content/hasEmployee> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isRelatedTo> ;
                                                        owl:inverseOf <http://www.w3id.org/arco/ontology/content/hasEmployer> ;
                                                        rdfs:comment "This property links an employer to its employee."@en .


###  http://www.w3id.org/arco/ontology/content/hasEmployer
<http://www.w3id.org/arco/ontology/content/hasEmployer> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isRelatedTo> ;
                                                        rdfs:comment "This property links an employee to its employer."@en .


###  http://www.w3id.org/arco/ontology/content/hasEnvironment
<http://www.w3id.org/arco/ontology/content/hasEnvironment> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasSociologicalCharacteristic> ;
                                                           rdfs:comment "This property links an agent to an environment."@en .


###  http://www.w3id.org/arco/ontology/content/hasEthnicGroup
<http://www.w3id.org/arco/ontology/content/hasEthnicGroup> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasSociologicalCharacteristic> ;
                                                           owl:inverseOf <http://www.w3id.org/arco/ontology/content/isEthnicGroupOf> ;
                                                           rdfs:comment "This property links an agent to an ethnic group."@en .


###  http://www.w3id.org/arco/ontology/content/hasEthnicOrigin
<http://www.w3id.org/arco/ontology/content/hasEthnicOrigin> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasOrigin> ;
                                                            owl:inverseOf <http://www.w3id.org/arco/ontology/content/isEthnicOriginOf> ;
                                                            rdfs:comment "This property defines the origin an agent has from an ethnic point of view."@en .


###  http://www.w3id.org/arco/ontology/content/hasFictionalType
<http://www.w3id.org/arco/ontology/content/hasFictionalType> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasType> ;
                                                             owl:inverseOf <http://www.w3id.org/arco/ontology/content/isFictionalType> ;
                                                             rdfs:comment "This property defines fictional character’s type as fictional or non fictional."@en .


###  http://www.w3id.org/arco/ontology/content/hasGeographicOrigin
<http://www.w3id.org/arco/ontology/content/hasGeographicOrigin> rdf:type owl:ObjectProperty ;
                                                                rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasOrigin> ;
                                                                owl:inverseOf <http://www.w3id.org/arco/ontology/content/isGeographicOriginOf> ;
                                                                rdfs:comment "This property defines the origin an agent has from a geographic point of view."@en .


###  http://www.w3id.org/arco/ontology/content/hasOrigin
<http://www.w3id.org/arco/ontology/content/hasOrigin> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasSociologicalCharacteristic> ;
                                                      rdfs:comment "This property defines the origin an agent has."@en .


###  http://www.w3id.org/arco/ontology/content/hasParent
<http://www.w3id.org/arco/ontology/content/hasParent> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasRelative> ;
                                                      rdfs:comment "This property link an agent that is child to another agent."@en .


###  http://www.w3id.org/arco/ontology/content/hasPartIn
<http://www.w3id.org/arco/ontology/content/hasPartIn> rdf:type owl:ObjectProperty ;
                                                      rdfs:comment "To be involved in, to be a factor in."@en .


###  http://www.w3id.org/arco/ontology/content/hasProfession
<http://www.w3id.org/arco/ontology/content/hasProfession> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasSociologicalCharacteristic> ;
                                                          rdfs:comment "This property links an agent to a profession."@en .


###  http://www.w3id.org/arco/ontology/content/hasRelative
<http://www.w3id.org/arco/ontology/content/hasRelative> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isRelatedTo> ;
                                                        rdf:type owl:SymmetricProperty ;
                                                        rdfs:comment "This property links two agents connected by blood or marriage."@en .


###  http://www.w3id.org/arco/ontology/content/hasRole
<http://www.w3id.org/arco/ontology/content/hasRole> rdf:type owl:ObjectProperty ;
                                                    owl:inverseOf <http://www.w3id.org/arco/ontology/content/isRole> ;
                                                    rdfs:comment "This property defines the role of character in a play."@en .


###  http://www.w3id.org/arco/ontology/content/hasSocialOrigin
<http://www.w3id.org/arco/ontology/content/hasSocialOrigin> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/hasOrigin> ;
                                                            rdf:type owl:FunctionalProperty ;
                                                            rdfs:comment "This property defines the origin an agent has from a social point of view."@en .


###  http://www.w3id.org/arco/ontology/content/hasSociologicalCharacteristic
<http://www.w3id.org/arco/ontology/content/hasSociologicalCharacteristic> rdf:type owl:ObjectProperty ;
                                                                          owl:inverseOf <http://www.w3id.org/arco/ontology/content/isSociologicalCharacteristicOf> ;
                                                                          rdfs:comment "This property defines a sociological characteristic that can be described as belonging to an agent."@en .


###  http://www.w3id.org/arco/ontology/content/hasSpinOff
<http://www.w3id.org/arco/ontology/content/hasSpinOff> rdf:type owl:ObjectProperty ;
                                                       owl:inverseOf <http://www.w3id.org/arco/ontology/content/isSpinOffOf> ;
                                                       rdfs:comment "This property links a TV serie to another TV serie in which a secondary character in the first show becomes the protagonist."@en .


###  http://www.w3id.org/arco/ontology/content/hasType
<http://www.w3id.org/arco/ontology/content/hasType> rdf:type owl:ObjectProperty ;
                                                    owl:inverseOf <http://www.w3id.org/arco/ontology/content/isType> ;
                                                    rdfs:comment "This property defines the typology of character depicted as human, animal or supernatural one."@en .


###  http://www.w3id.org/arco/ontology/content/isAuthorOf
<http://www.w3id.org/arco/ontology/content/isAuthorOf> rdf:type owl:ObjectProperty ;
                                                       rdfs:comment "This property links a creator or originator to its written work."@en .


###  http://www.w3id.org/arco/ontology/content/isBasedOn
<http://www.w3id.org/arco/ontology/content/isBasedOn> rdf:type owl:ObjectProperty ,
                                                               owl:FunctionalProperty ,
                                                               owl:InverseFunctionalProperty ;
                                                      rdfs:comment "This property defines a character that is based on or inspired to a real or invented person, animal or supernatural being."@en .


###  http://www.w3id.org/arco/ontology/content/isCharacterOf
<http://www.w3id.org/arco/ontology/content/isCharacterOf> rdf:type owl:ObjectProperty .


###  http://www.w3id.org/arco/ontology/content/isCharacterRole
<http://www.w3id.org/arco/ontology/content/isCharacterRole> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isType> ;
                                                            rdfs:comment "This property defines protagonist, antagonist and secondary as character’s role."@en .


###  http://www.w3id.org/arco/ontology/content/isCharacterScope
<http://www.w3id.org/arco/ontology/content/isCharacterScope> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isType> ;
                                                             rdfs:comment "This property defines universal good, universal bad and self good as character’s scope."@en .


###  http://www.w3id.org/arco/ontology/content/isCharacterStatus
<http://www.w3id.org/arco/ontology/content/isCharacterStatus> rdf:type owl:ObjectProperty ;
                                                              rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isType> ;
                                                              rdfs:comment "This property defines leading, opposing, and supporting as character’s status."@en .


###  http://www.w3id.org/arco/ontology/content/isCharacterType
<http://www.w3id.org/arco/ontology/content/isCharacterType> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isType> ;
                                                            rdfs:comment "This property defines hero, antihero, and villain as character’s type."@en .


###  http://www.w3id.org/arco/ontology/content/isDefinedBy
<http://www.w3id.org/arco/ontology/content/isDefinedBy> rdf:type owl:ObjectProperty ;
                                                        rdfs:comment "This property defines the meaning that is attached to a term."@en .


###  http://www.w3id.org/arco/ontology/content/isEnvironmentOf
<http://www.w3id.org/arco/ontology/content/isEnvironmentOf> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isSociologicalCharacteristicOf> ;
                                                            rdfs:comment "This property links an environment to an agent."@en .


###  http://www.w3id.org/arco/ontology/content/isEthnicGroupOf
<http://www.w3id.org/arco/ontology/content/isEthnicGroupOf> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isSociologicalCharacteristicOf> ;
                                                            rdfs:comment "This property links an ethnic group to an agent."@en .


###  http://www.w3id.org/arco/ontology/content/isEthnicOriginOf
<http://www.w3id.org/arco/ontology/content/isEthnicOriginOf> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isOriginOf> ;
                                                             rdfs:comment "This property defines the origin referring to an agent from an ethnic point of view."@en .


###  http://www.w3id.org/arco/ontology/content/isFictionalType
<http://www.w3id.org/arco/ontology/content/isFictionalType> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isType> ;
                                                            rdfs:comment "This property defines fictional and non fictional as character’s fictional type."@en .


###  http://www.w3id.org/arco/ontology/content/isGeographicOriginOf
<http://www.w3id.org/arco/ontology/content/isGeographicOriginOf> rdf:type owl:ObjectProperty ;
                                                                 rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isOriginOf> ;
                                                                 rdfs:comment "This property defines the origin referring to an agent from a geographic point of view."@en .


###  http://www.w3id.org/arco/ontology/content/isKilledBy
<http://www.w3id.org/arco/ontology/content/isKilledBy> rdf:type owl:ObjectProperty ;
                                                       owl:inverseOf <http://www.w3id.org/arco/ontology/content/kills> ;
                                                       rdfs:comment "This property links two agents in which the first one is killed by the second one."@en .


###  http://www.w3id.org/arco/ontology/content/isOriginOf
<http://www.w3id.org/arco/ontology/content/isOriginOf> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isSociologicalCharacteristicOf> ;
                                                       rdfs:comment "This property defines the origin referring to an agent."@en .


###  http://www.w3id.org/arco/ontology/content/isPlayedBy
<http://www.w3id.org/arco/ontology/content/isPlayedBy> rdf:type owl:ObjectProperty ;
                                                       owl:inverseOf <http://www.w3id.org/arco/ontology/content/plays> ;
                                                       rdfs:comment "This property defines a character that is interpreted by an actor."@en .


###  http://www.w3id.org/arco/ontology/content/isProfessionOf
<http://www.w3id.org/arco/ontology/content/isProfessionOf> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isSociologicalCharacteristicOf> ;
                                                           rdfs:comment "This property link a profession to an agent."@en .


###  http://www.w3id.org/arco/ontology/content/isRelatedTo
<http://www.w3id.org/arco/ontology/content/isRelatedTo> rdf:type owl:ObjectProperty ,
                                                                 owl:SymmetricProperty ;
                                                        rdfs:comment "This property defines a personal relations between two agents."@en .


###  http://www.w3id.org/arco/ontology/content/isRole
<http://www.w3id.org/arco/ontology/content/isRole> rdf:type owl:ObjectProperty ;
                                                   rdfs:comment "This property defines a character by its role in a play."@en .


###  http://www.w3id.org/arco/ontology/content/isSocialOriginOf
<http://www.w3id.org/arco/ontology/content/isSocialOriginOf> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://www.w3id.org/arco/ontology/content/isOriginOf> ;
                                                             rdfs:comment "This property defines the origin referring to an agent from a social point of view."@en .


###  http://www.w3id.org/arco/ontology/content/isSociologicalCharacteristicOf
<http://www.w3id.org/arco/ontology/content/isSociologicalCharacteristicOf> rdf:type owl:ObjectProperty ;
                                                                           rdfs:comment "This property defines a sociological characteristic that refers to an agent." .


###  http://www.w3id.org/arco/ontology/content/isSpinOffOf
<http://www.w3id.org/arco/ontology/content/isSpinOffOf> rdf:type owl:ObjectProperty ;
                                                        rdfs:comment "This property links a TV serie to another TV serie in which the protagonist played a secondary role."@en .


###  http://www.w3id.org/arco/ontology/content/isType
<http://www.w3id.org/arco/ontology/content/isType> rdf:type owl:ObjectProperty ;
                                                   rdfs:comment "This property defines human, animal or supernatural being as typology of character."@en .


###  http://www.w3id.org/arco/ontology/content/kills
<http://www.w3id.org/arco/ontology/content/kills> rdf:type owl:ObjectProperty ;
                                                  rdfs:comment "This property links two agents in which the first one kills the second one."@en .


###  http://www.w3id.org/arco/ontology/content/plays
<http://www.w3id.org/arco/ontology/content/plays> rdf:type owl:ObjectProperty ;
                                                  rdfs:comment "This property defines an actor that interprets a character."@en .


#################################################################
